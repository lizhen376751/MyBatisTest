<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lizhen.mybatis.mapper.UserMapper">
	<select id="getUser" parameterType="int" resultType="com.lizhen.mybatis.entity.User">
		SELECT *
		FROM users where id =#{id}
	</select>
	
</mapper>
